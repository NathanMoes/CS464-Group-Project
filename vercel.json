{
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "functions": {
    "api/index.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/**",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept"
        }
      ]
    }
  ]
}
